{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",

    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-soap": "*",
        "barryvdh/laravel-ide-helper": "^2.1",
        "doctrine/dbal": "^2.5",
        "econea/nusoap": "^0.9.10",
        "facade/ignition": "^2.3.6",
        "google/apiclient": "^2.0",
        "jakubkulhan/chrome-devtools-protocol": "^1.0",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laminas/laminas-soap": "^2.8",
        "laravel/framework": "^8.0",
        "laravel/helpers": "^1.1",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.0",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "^2.25.0",
        "phpoffice/phpspreadsheet": "^1.9",
        "phpoffice/phpword": "^0.18.2",
        "predis/predis": "^1.1",
        "rats/zkteco": "^002.0",
        "realrashid/sweet-alert": "^3.0",
        "revolution/laravel-google-sheets": "^5.7",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/browsershot": "^3.40",
        "symfony/dom-crawler": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "classmap": [
            "database",
            "KGS/database"
        ],
        "psr-4": {
            "App\\": "app/",
            "KGS\\": "KGS/app/",
            "Make\\": "Make"
        },

        "files": [ "app/Support/helpers.php" ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],

        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
